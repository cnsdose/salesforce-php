{
    "name": "cnsdose/salesforce-php",
    "description": "Salesforce RESTful client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Frederick Zhang",
            "email": "frederick888@tsundere.moe"
        }
    ],
    "autoload": {
        "psr-4": {
            "CNSDose\\Salesforce\\Console\\": "src/Console",
            "CNSDose\\Salesforce\\Exceptions\\": "src/Exceptions",
            "CNSDose\\Salesforce\\Models\\": "src/Models",
            "CNSDose\\Salesforce\\Providers\\": "src/Providers",
            "CNSDose\\Salesforce\\Support\\": "src/Support",
            "CNSDose\\Salesforce\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.0",
        "ext-json": "*",
        "ext-soap": "*",
        "ext-xmlreader": "*",
        "cnsdose/cnsdose-standards": "dev-master",
        "guzzlehttp/guzzle": "^6.3",
        "firebase/php-jwt": "^5.0",
        "myclabs/php-enum": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7",
        "vlucas/phpdotenv": "^2.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:cnsd/cnsdose-standards.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "CNSDose\\Salesforce\\Providers\\SalesforceProvider"
            ]
        }
    }
}
